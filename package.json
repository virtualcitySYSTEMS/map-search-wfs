{
  "name": "@vcmap/search-wfs",
  "version": "2.0.2",
  "description": "wfs search extension",
  "type": "module",
  "main": "dist/index.ts",
  "scripts": {
    "prepublishOnly": "vcmplugin build",
    "build": "vcmplugin build",
    "bundle": "vcmplugin bundle",
    "start": "vcmplugin serve",
    "preview": "vcmplugin preview",
    "buildStagingApp": "vcmplugin buildStagingApp",
    "lint:js": "eslint . --ext .vue,.js,.cjs,.mjs,.ts,.cts,.mts",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:prettier",
    "type-check": "vue-tsc --noEmit",
    "format": "prettier --write --list-different . && npm run lint:js -- --fix",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "author": "Virtual City Systems",
  "license": "MIT",
  "keywords": [
    "vcmap",
    "plugin"
  ],
  "files": [
    "src/",
    "dist/",
    "plugin-assets/",
    "LICENSE.md",
    "README.md",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./dist": "./dist/index.js"
  },
  "eslintIgnore": [
    "node_modules",
    "dist",
    "plugin-assets"
  ],
  "prettier": "@vcsuite/eslint-config/prettier.js",
  "peerDependencies": {
    "@vcmap-cesium/engine": "^11.0.2",
    "@vcmap/core": "^6.0.4",
    "@vcmap/ui": "^6.0.6",
    "ol": "^10.2.1",
    "underscore.template": "~0.1.7",
    "vue": "~3.4.38",
    "vuetify": "^3.7.3"
  },
  "devDependencies": {
    "@vcmap/plugin-cli": "^4.0.1",
    "@vcsuite/eslint-config": "^3.0.8",
    "@vitest/coverage-v8": "^2.1.4",
    "jest-canvas-mock": "^2.5.2",
    "jsdom": "^24.1.1",
    "resize-observer-polyfill": "^1.5.1",
    "typescript": "^5.6.3",
    "vite-plugin-vuetify": "^2.0.3",
    "vitest": "^2.1.4",
    "vue-tsc": "^2.1.10"
  },
  "overrides": {
    "esbuild": "^0.25.0",
    "@zip.js/zip.js": "2.7.68"
  },
  "dependencies": {
    "@vcsuite/check": "^1.1.2",
    "@vcsuite/logger": "^1.0.1",
    "@vcsuite/parsers": "^1.0.3",
    "underscore.template": "~0.1.7"
  },
  "mapVersion": "^6.0"
